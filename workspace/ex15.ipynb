    "from sys import argv\n",
    "\n",
    "script, filename = argv\n",
    "\n",
    "txt = open (filename)\n",
    "\n",
    "print (\"Here's your file %r\" % filename)\n",
    "\n",
    "print (txt.read())\n",
    "\n",
    "print (\"Type the filename again:\")\n",
    "\n",
    "file_name = input(\">\")\n",
    "\n",
    "txt_again = open (file_name)\n",
    "\n",
    "print (txt_again.read())\n"
   ]
  }
